    "@types/aws4": "^1.11.6",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@pact-foundation/pact-core": "^15.2.1",
    "@pact-foundation/pact-node": "^10.17.7",
    "mocha": "^10.2.0"
  }
