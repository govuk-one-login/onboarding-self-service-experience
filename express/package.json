{
  "name": "di-onboarding-self-service",
  "version": "1.0.0",
  "description": "Self-service",
  "main": "index.js",
  "scripts": {
    "start": "node -r 'dotenv/config' dist/app.js",
    "local": "npm run build && node -r 'dotenv/config' dist/app.js",
    "dev": "COGNITO_CLIENT=${COGNITO_CLIENT:-StubCognitoClient} LAMBDA_FACADE=${LAMBDA_FACADE:-StubLambdaFacade} nodemon --config nodemon.json -r 'dotenv/config' src/app.ts",
    "buildts": "tsc -p .",
    "buildsass": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js assets/css/app.scss dist/assets/app.css",
    "buildjs": "uglifyjs --verbose node_modules/govuk-frontend/govuk/all.js -o dist/assets/govuk-all.js && uglifyjs --verbose assets/javascripts/cookies.js -o dist/assets/cookies.js && cp -v -r assets/javascripts/show-password.js ./dist/assets && cp -v -r assets/javascripts/show-password-two-fields.js ./dist/assets",
    "buildgovuk": "cp -v -r node_modules/govuk-frontend/govuk/assets/* ./dist/assets && echo \"Done!\"",
    "buildlocalimages": "cp -v -r assets/images/* ./dist/assets/images",
    "build": "npm run buildts && npm run buildsass && npm run buildjs && npm run buildgovuk && npm run buildlocalimages",
    "test": "jest",
    "cucumber": "./node_modules/.bin/cucumber-js --require-module ts-node/register --require features/support/test-setup.ts --require features/support/steps/**/*.ts --publish-quiet --tags 'not @wip'",
    "cucumber-report": "npm run cucumbers -- --format html:test-report.html"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.80.0",
    "@aws-sdk/util-dynamodb": "^3.100.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.4",
    "aws-jwt-verify": "^3.1.0",
    "axios": "^0.24.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "govuk-frontend": "^3.13.1",
    "node-sass-package-importer": "^5.3.2",
    "nodemon": "^2.0.16",
    "nunjucks": "^3.2.3",
    "rfc822-validate": "^1.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.0.0-rc.2",
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "@types/nunjucks": "^3.2.0",
    "@types/puppeteer": "^5.4.4",
    "@types/uuid": "^8.3.3",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "node-sass": "^6.0.1",
    "puppeteer": "^13.1.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "uglify": "^0.1.5"
  },
  "author": "",
  "license": "MIT"
}
