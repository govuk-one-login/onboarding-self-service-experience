{% extends "layouts/account.njk" %}

{% set sidenavActiveItem = "your-services-sidenav" %}
{% set pageTitle = "Your services" %}

{% block mainContent %}
  <h1 class="govuk-heading-l govuk-!-margin-top-3">
    Your services
  </h1>

  {% for service in services %}
    <p>
      <a class="govuk-body-l govuk-!-font-weight-bold govuk-link--no-visited-state"
         href="/services/{{ service.id }}/clients">{{ service.serviceName }}</a>
    </p>
    <hr class="govuk-section-break govuk-section-break--visible govuk-section-break--l">
  {% endfor %}
  
{% endblock %}
