{
  "name": "gds-di-self-service-common",
  "description": "DI self-service monorepo",
  "workspaces": [
    "express",
    "backend/api"
  ],
  "scripts": {
    "build": "npm run build -w express",
    "build-docker": "npm run build-docker -w express --",
